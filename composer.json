{
  "name": "alnv/contao-geocoding",
  "type": "contao-bundle",
  "description": "",
  "authors": [
    {
      "name": "Alexander Naumov",
      "homepage": "https://www.alexandernaumov.de",
      "role": "Developer"
    }
  ],
  "autoload": {
    "psr-4": {
      "Alnv\\ContaoGeoCodingBundle\\": "src/"
    },
    "classmap": [
      "src/Resources/contao/"
    ],
    "exclude-from-classmap": [
      "src/Resources/contao/config/",
      "src/Resources/contao/dca/",
      "src/Resources/contao/languages/"
    ]
  },
  "require": {
    "contao/core-bundle":"~4.9"
  },
  "config": {
    "preferred-install": "dist"
  },
  "extra": {
    "contao": {
      "sources": {
        "": "system/modules/contao-geocoding-bundle"
      }
    },
    "contao-manager-plugin": "Alnv\\ContaoGeoCodingBundle\\ContaoManager\\Plugin"
  }
}
